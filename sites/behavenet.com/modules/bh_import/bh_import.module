    if (isset($import_tradenames[strtolower($tradename)])) {
      dpr ("Error: $tradename is duplicated in the import dataset.");
    }
    elseif (isset($existing_tradenames[strtolower($tradename)])) {
      dpr("Error: $tradename already exists in the system.");
    }
    else {
      $import_tradenames[] = array(
        'name' => $tradename,
        'generic_id' => $existing_generics[strtolower($generic)],
